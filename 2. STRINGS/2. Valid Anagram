#include<bits/stdc++.h>
using namespace std;

bool isAnagram(string s, string t){
    
    sort(s.begin(), s.end());
    sort(t.begin(), t.end());
    return s == t;
}

int main() {
    
    string s = "anagram";
    string t = "nagaram";
    if (isAnagram(s, t)){
        cout<<"Yes";
    }
    else{
        cout<<"Not";
    }
    return 0;
}

OUTPUT : 
Yes
